{
  "name": "landa-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check '**/*.{ts,tsx,js,jsx}' --ignore-path .prettierignore --no-error-on-unmatched-pattern",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js}\"",
    "test-all": "yarn run check-types && yarn run check-lint && yarn run build"
  },
  "dependencies": {
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3"
  }
}
